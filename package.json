{
  "name": "csrf-login",
  "version": "1.3.3",
  "description": "Login from command line to the websites that use CSRF protection",
  "main": "src/csrf-login.js",
  "scripts": {
    "test": "eslint src",
    "style": "jscs src/*.js --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahmutov/csrf-login.git"
  },
  "keywords": [
    "login",
    "cli",
    "command",
    "line",
    "csrf",
    "token",
    "test",
    "utility"
  ],
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahmutov/csrf-login/issues"
  },
  "homepage": "https://github.com/bahmutov/csrf-login#readme",
  "dependencies": {
    "bluebird": "2.9.34",
    "check-more-types": "1.8.2",
    "cheerio": "0.19.0",
    "debug": "2.2.0",
    "first-existing": "1.0.0",
    "lazy-ass": "0.5.8",
    "nconf": "0.7.2",
    "request": "2.60.0"
  },
  "devDependencies": {
    "eslint": "1.1.0",
    "jscs": "2.1.0",
    "pre-git": "0.6.2"
  },
  "pre-commit": [
    "npm test",
    "npm run style"
  ],
  "post-commit": "npm version",
  "eslintConfig": {
    "env": {
      "node": true
    },
    "rules": {
      "no-unused-expressions": 2,
      "no-undef": 2
    },
    "globals": {
      "la": true
    }
  }
}
